<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.24"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkImage" id="apps_search">
    <property name="width_request">47</property>
    <property name="height_request">30</property>
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="pixbuf">graphics/search_button.png</property>
  </object>
  <object class="GtkImage" id="file_search">
    <property name="width_request">47</property>
    <property name="height_request">30</property>
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="pixbuf">graphics/search_button.png</property>
  </object>
  <object class="GtkWindow" id="window1">
    <property name="width_request">350</property>
    <property name="height_request">500</property>
    <property name="can_focus">False</property>
    <property name="resizable">False</property>
    <signal name="destroy-event" handler="gtk_main_quit" swapped="no"/>
    <child>
      <object class="GtkNotebook" id="notebook1">
        <property name="visible">True</property>
        <property name="app_paintable">True</property>
        <property name="can_focus">True</property>
        <property name="tab_pos">bottom</property>
        <property name="tab_border">15</property>
        <property name="tab_hborder">15</property>
        <property name="tab_vborder">15</property>
        <child>
          <object class="GtkFrame" id="apps_frame">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label_xalign">0</property>
            <property name="shadow_type">none</property>
            <child>
              <object class="GtkVPaned" id="vpaned1">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="position_set">True</property>
                <child>
                  <object class="GtkHPaned" id="hpaned1">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <child>
                      <object class="GtkEntry" id="apps_searchbox">
                        <property name="width_request">200</property>
                        <property name="height_request">40</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="max_length">30</property>
                        <property name="invisible_char">•</property>
                        <property name="width_chars">30</property>
                        <property name="invisible_char_set">True</property>
                        <property name="primary_icon_stock">gtk-edit</property>
                        <property name="primary_icon_activatable">True</property>
                        <property name="secondary_icon_activatable">False</property>
                        <property name="primary_icon_sensitive">True</property>
                        <property name="secondary_icon_sensitive">True</property>
                        <property name="primary_icon_tooltip_text" translatable="yes">Search Applications</property>
                        <property name="primary_icon_tooltip_markup" translatable="yes">Search Applications</property>
                        <signal name="activate" handler="on_apps_searchbox_activate" swapped="no"/>
                      </object>
                      <packing>
                        <property name="resize">False</property>
                        <property name="shrink">False</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="apps_searchbtn">
                        <property name="label" translatable="yes">

</property>
                        <property name="width_request">38</property>
                        <property name="height_request">30</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="use_action_appearance">False</property>
                        <property name="image">apps_search</property>
                        <property name="image_position">top</property>
                      </object>
                      <packing>
                        <property name="resize">False</property>
                        <property name="shrink">False</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="resize">False</property>
                    <property name="shrink">False</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="apps_display_result">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">Result</property>
                    <property name="selectable">True</property>
                  </object>
                  <packing>
                    <property name="resize">True</property>
                    <property name="shrink">True</property>
                  </packing>
                </child>
              </object>
            </child>
            <child type="label">
              <object class="GtkLabel" id="apps_frame_title">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="xpad">148</property>
                <property name="ypad">12</property>
                <property name="label" translatable="yes">Apps</property>
                <property name="ellipsize">start</property>
                <property name="width_chars">11</property>
                <property name="single_line_mode">True</property>
                <property name="max_width_chars">30</property>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="tabs_apps">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Apps</property>
          </object>
          <packing>
            <property name="tab_fill">False</property>
          </packing>
        </child>
        <child>
          <object class="GtkFrame" id="files_frame">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label_xalign">0</property>
            <property name="shadow_type">none</property>
            <child>
              <object class="GtkVPaned" id="vpaned2">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <child>
                  <object class="GtkHPaned" id="hpaned2">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <child>
                      <object class="GtkEntry" id="files_searchbox">
                        <property name="width_request">200</property>
                        <property name="height_request">40</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="max_length">30</property>
                        <property name="invisible_char">•</property>
                        <property name="width_chars">30</property>
                        <property name="invisible_char_set">True</property>
                        <property name="primary_icon_stock">gtk-edit</property>
                        <property name="primary_icon_activatable">True</property>
                        <property name="secondary_icon_activatable">False</property>
                        <property name="primary_icon_sensitive">True</property>
                        <property name="secondary_icon_sensitive">True</property>
                        <property name="primary_icon_tooltip_text" translatable="yes">Search Applications</property>
                        <property name="primary_icon_tooltip_markup" translatable="yes">Search Applications</property>
                      </object>
                      <packing>
                        <property name="resize">False</property>
                        <property name="shrink">False</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="files_searchbtn">
                        <property name="label">

</property>
                        <property name="width_request">38</property>
                        <property name="height_request">30</property>
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">True</property>
                        <property name="use_action_appearance">False</property>
                        <property name="image">file_search</property>
                      </object>
                      <packing>
                        <property name="resize">False</property>
                        <property name="shrink">False</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="resize">False</property>
                    <property name="shrink">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="file_display_result">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">label</property>
                  </object>
                  <packing>
                    <property name="resize">True</property>
                    <property name="shrink">True</property>
                  </packing>
                </child>
              </object>
            </child>
            <child type="label">
              <object class="GtkLabel" id="files_frame_title">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="xpad">148</property>
                <property name="ypad">12</property>
                <property name="label" translatable="yes">Files</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="tabs_files">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Files</property>
          </object>
          <packing>
            <property name="position">1</property>
            <property name="tab_fill">False</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
